{
  "name": "pwa-starter-kit",
  "version": "0.8.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/pwa-starter-kit",
  "scripts": {
    "start": "polymer serve",
    "build": "npm run build:prpl-server && npm run build:static",
    "start:webpack": "webpack-dev-server --open --mode development",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "build:webpack": "rm -rf dist && webpack --config webpack.config.js",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "serve:webpack": "polymer serve --port 3000 dist",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm",
    "test:sauce": "polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@material/mwc-button": "^0.1.2",
    "@material/mwc-checkbox": "^0.1.2",
    "@polymer/lit-element": "^0.5.2",
    "@polymer/paper-input": "^3.0.0-pre.21",
    "@vaadin/vaadin-button": "^2.1.0-beta2",
    "@vaadin/vaadin-checkbox": "^2.2.0-beta2",
    "@vaadin/vaadin-combo-box": "^4.1.0-beta2",
    "@vaadin/vaadin-date-picker": "^3.2.0-beta2",
    "@vaadin/vaadin-dropdown-menu": "^1.1.0-beta2",
    "@vaadin/vaadin-icons": "^4.2.0-beta2",
    "@vaadin/vaadin-item": "^2.1.0-beta2",
    "@vaadin/vaadin-list-box": "^1.1.0-beta2",
    "@vaadin/vaadin-notification": "^1.1.0-beta2",
    "@vaadin/vaadin-progress-bar": "^1.1.0-beta2",
    "@vaadin/vaadin-text-field": "^2.1.0-beta2",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "lit-html": "^0.10.2",
    "pwa-helpers": "^0.8.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.2.3",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.1.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.7.4",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.1.0",
    "puppeteer": "^1.4.0",
    "wct-browser-legacy": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.4",
    "html-webpack-plugin": "^3.2.0",
    "babel-loader": "^7.1.4"
  }
}